  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.defaultto": "^4.14.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.isempty": "^4.4.0",
    "lodash.negate": "^3.0.2",
    "normalize-path": "^3.0.0",
    "split2": "^4.1.0"
  },
